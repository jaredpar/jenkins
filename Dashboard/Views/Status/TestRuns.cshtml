@model List<Dashboard.Models.TestRunComparison>

@{
    ViewBag.Title = $"Test Runs Summary";

    var values = string.Join(
        ";",
        Model.Select(x => $"{x.Date.ToLocalTime().ToString("yyyy-MM-dd")},{x.LegacyTime.TotalMinutes},{x.ChunkOnlyTime.TotalMinutes},{x.FullCacheTime.TotalMinutes}"));
}

<div id="testrun_comparison_chart" style="width: 900px; height: 500px" data-values="@values"></div>

@section scripts {
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript" src="@Url.Content("/Scripts/testrun-data.js")" ></script>
}
